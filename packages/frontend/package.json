    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "blockies-ts": "^1.0.0",
    "deepmerge": "^4.2.2",
    "ethers": "5.6.9",
    "framer-motion": "^6",
    "lodash": "4.17.21",
    "next": "^12.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
